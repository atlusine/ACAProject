CREATE DATABASE ACA_Studentsl;

USE ACA_Students;

CREATE TABLE Payments
(
ID INT AUTO_INCREMENT PRIMARY KEY,
LastName varchar(255),
FirstName varchar(255),
);
USE aca_students
CREATE TABLE payment (LastName varchar(255), FirstName varchar(255), Payment_method varchar(255), Card_number int)
SELECT FROM payment
CREATE TABLE students_Grades AS SELECT LastName, FirstName FROM Payment
